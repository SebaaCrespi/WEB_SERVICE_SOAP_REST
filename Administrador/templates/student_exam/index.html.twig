{% extends 'base.html.twig' %}

{% block title %}StudentExam index{% endblock %}

{% block body %}
    <h1>StudentExam index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Note</th>
                <th>IsActive</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for student_exam in student_exams %}
            <tr>
                <td>{{ student_exam.id }}</td>
                <td>{{ student_exam.note }}</td>
                <td>{{ student_exam.isActive ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_student_exam_show', {'id': student_exam.id}) }}">show</a>
                    <a href="{{ path('app_student_exam_edit', {'id': student_exam.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_student_exam_new') }}">Create new</a>
{% endblock %}
